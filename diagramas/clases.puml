@startuml Diagrama de clases

abstract class Vehiculo {
    # matricula: string
    # estado : Estado
    # odometro : Odometro 
    + setters_y_getters()
}

class Garage {
    - autos : []

    + setters_y_getters()
}


class Mantenimiento {
    - costo : number
    - fecha : number
    - matricula : string
}

class Reserva {
    - cant_dias: number
    - vehiculo: Vehiculo

    + setters_y_getters()
    + crearReserva(fecha_inicio: number, fecha_fin: number, vehiculo: Vehiculo)
}

enum Estado {
    + realizarMantenimiento(vehiculo: Vehiculo)
}

class Compacto {

}
class Sedán {

}
class SUV {

}

' Relaciones

Vehiculo  *-r- Estado
Reserva -r- Garage
Garage <-d- Vehiculo
Mantenimiento *-r- Vehiculo 

Vehiculo  <|-d- Compacto
Vehiculo  <|-d- Sedán
Vehiculo <|-d- SUV


@enduml