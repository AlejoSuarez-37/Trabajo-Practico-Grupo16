@startuml Diagrama de clases

class Cliente {
    - vehiculos: Array<Vehiculo>;
    - reservas: Array<Evento>;
    - mantenimientos: Array<Evento>;

    + comprarCompacto(matricula: string)
    + comprarSUV(matricula: string)
    + comprarSedán(matricula: string)

    + crearReserva()
    + devolverVehiculo()

    + realizarMantenimiento()
    + terminarMantenimiento()

    + limpiarVehiculo()
}

abstract class Vehiculo {
    - matricula: string
    - necesitaLimpieza: boolean
    
    + abstract obtenerTarifa(dias: number, kilometros: number)
    + abstract obtenerTarifaMantenimiento(dias: number)
}

class Compacto {

}
class Sedán {

}
class SUV {

}

class Evento {
    - cant_dias: number
    - tipoVehiculo: string
    - vehiculo: Vehiculo
    - ticket: number

    + setters_y_getters()

    - calcularCantDias()
}

class Calendario {
    + revisarCalendario()
    + estaDisponibleHoy()
}


' Relaciones

Cliente *-- Vehiculo
Cliente *-- Evento

Evento *-r- Vehiculo

Vehiculo <|-- Compacto
Vehiculo <|-- Sedán
Vehiculo <|-- SUV

@enduml