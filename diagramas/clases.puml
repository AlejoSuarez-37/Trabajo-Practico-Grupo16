@startuml Diagrama de clases
class Garage {
    - vehiculos: Map<string,Vehiculo>;
    - reservas: Set<Evento>;
    - mantenimientos: Set<Evento>;
    
    + comprarVehiculo(vehiculo: Vehiculo)
    + setters_y_getters()
    + cambiarTemporada()
}

class Cliente {
    + reservar()
    + devolverVehiculo()
}

class Mecanico {
    + realizarMantenimiento()
}

class Lavanderia {
    + limpiarVehiculo()
}

class Estado {
    - kilometrosRecorridos: number
    - ultimoMantenimiento: Date
    - alquileresCompletados: number
    - necesitaLimpieza: boolean
    
    + setters_y_getters()
    + necesitaMantenimiento()
}

abstract class Vehiculo {
    - matricula: string
    - estado: Estado

    # temporada: Temporada

    + setters_y_getters()
    + abstract obtenerTarifa(dias: number, kilometros: number)
    + abstract obtenerTarifaMantenimiento(dias: number)
}

class Compacto {

}
class Sedán {

}
class SUV {

}

class Evento {
    - fechaInicio: Date
    - fechaFin: Date
    - cant_dias: number
    - vehiculo: Vehiculo

    + setters_y_getters()

    - calcularCantDias()
}

class Calendario {
    static revisarCalendario()
    static estaDisponibleHoy()
}

class OperacionesInvalidas {
    static estaDisponible()
    static vehiculoEnStock()
}

class DisparadorMantenimiento {
    static chequearVehiculo()
}

interface Temporada {
    modificador:number
    + abstract getModificador()
}
class TemporadaBaja implements Temporada{
    # modificador:number
    + getModificador()
}
class TemporadaMedia implements Temporada{
    # modificador:number
    + getModificador()
}
class TemporadaAlta implements Temporada{
    # modificador:number
    + getModificador()
}


' Relaciones


Cliente *-- Garage

Cliente *-r- DisparadorMantenimiento

Mecanico *-r- Garage
Lavanderia *-l- Garage

Garage *-d- Vehiculo
Garage *-d- Evento

Evento *-r- Vehiculo

Vehiculo <|-- Compacto
Vehiculo <|-- Sedán
Vehiculo <|-- SUV

Vehiculo *-r- Temporada
Vehiculo *-d- Estado

OperacionesInvalidas *-- Calendario


@enduml